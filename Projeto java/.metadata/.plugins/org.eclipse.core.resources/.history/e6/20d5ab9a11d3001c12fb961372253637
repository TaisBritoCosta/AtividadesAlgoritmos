package Heranca;

public class Animal {

	private String nome;
	private int idade;
	boolean correr;
	boolean somAnimal;	
	
	public void correr() {
	}
	public void som() {
	}
	public String getNome() {
		return nome;
    }
	public void setNome(String nome) {
		
	}
	public int getIdade() {
		return idade;
	}
	public void setIdade(int idade) {
		this.idade = idade;
	}
    


}
