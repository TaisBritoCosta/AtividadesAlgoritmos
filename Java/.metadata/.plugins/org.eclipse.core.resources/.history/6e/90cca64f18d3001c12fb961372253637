package Heranca;

public class Animal {

	public String nome;
	public int idade;
	public boolean correr;
	public boolean somAnimal;	
	
	public void correr() {
	}
	public void som() {
	}
	public String getNome() {
		return nome;
    }
	public void setNome(String nome) {
		
	}
	public int getIdade() {
		return idade;
	}
	public void setIdade(int idade) {
		this.idade = idade;
	}
    


}
