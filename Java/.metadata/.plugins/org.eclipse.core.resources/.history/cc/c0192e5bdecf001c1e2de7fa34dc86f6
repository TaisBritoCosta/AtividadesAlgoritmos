	System.out.println("Ola mundo");
	  int idade = 16;
	  int quantidadePessoas = 3;
	  	  
	  if (idade >=18 || quantidadePessoas >=2) {
	   System.out.println("Bem vindo"); 
   	  	  } else {
			  System.out.println("Infelizmente voce nao pode entrar");
		  }
}
    
    
    
	}


